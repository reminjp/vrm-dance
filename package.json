{
  "name": "vrm-dance",
  "version": "0.0.0",
  "private": true,
  "description": "My template repository for simple web apps written in TypeScript.",
  "repository": {
    "type": "git",
    "url": "https://github.com/rdrgn/vrm-dance.git"
  },
  "license": "UNLICENSED",
  "author": "remin",
  "scripts": {
    "build": "yarn run clean && webpack",
    "clean": "rimraf dist",
    "format": "yarn run sort-package-json && yarn run prettier && yarn run lint-fix",
    "gh-pages": "yarn build && gh-pages -d dist",
    "lint": "eslint --ext .js,.json,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "lint-fix": "eslint --fix --ext .js,.json,.jsx,.ts,.tsx --ignore-path .gitignore .",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.{css,html,js,json,jsx,md,ts,tsx}\"",
    "sort-package-json": "sort-package-json",
    "start": "webpack-dev-server --mode=development --open"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{?(s)css,htm?(l),json,md,y?(a)ml}": [
      "prettier --write",
      "git add"
    ],
    "*.{js?(x),ts?(x)}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@pixiv/three-vrm": "0.3.2",
    "prop-types": "15.7.2",
    "react": "16.13.0",
    "react-dom": "16.13.0",
    "react-resize-detector": "4.2.1",
    "react-rnd": "10.1.6",
    "react-split-pane": "0.1.89",
    "three": "0.114.0",
    "uuid": "7.0.2"
  },
  "devDependencies": {
    "@types/html-webpack-plugin": "3.2.2",
    "@types/prop-types": "15.7.3",
    "@types/react": "16.9.23",
    "@types/react-dom": "16.9.5",
    "@types/react-resize-detector": "4.2.0",
    "@types/webpack": "4.41.7",
    "@typescript-eslint/eslint-plugin": "2.23.0",
    "@typescript-eslint/parser": "2.23.0",
    "autoprefixer": "9.7.4",
    "css-loader": "3.4.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "fibers": "4.0.2",
    "gh-pages": "2.2.0",
    "html-webpack-plugin": "3.2.0",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "postcss-loader": "3.0.0",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "sort-package-json": "1.40.0",
    "style-loader": "1.1.3",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.3",
    "webpack": "4.42.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
}
